{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "linebreak-style": 0,
      "no-console": "off",
      "max-len": ["error", { "code": 150 }],
      "object-curly-newline": ["error", {
        "ImportDeclaration": "never",
        "ExportDeclaration": { "multiline": true, "minProperties": 3 }
      }]
    },
    "extends": "airbnb-base"
}